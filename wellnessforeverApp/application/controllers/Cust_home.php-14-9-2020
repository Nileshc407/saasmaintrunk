				$this->load->view('front/profile/profile_address', $data);
				
			} else {
				
				// var_dump($_POST);
				// die;
				
				
				
                /*********AMIT 08-11-2017*************************** */
                $phno = $this->input->post("phno");
                $Country = $this->input->post("country");
